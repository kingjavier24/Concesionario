<h5>BIENVENIDO VENDEDOR, REALICE SU PUBLICACION ACA.    </h5>

<section class="form-register">
    <h4>VENDE TU VEHICULO</h4>
    <form action="/vehiculo" method="POST" enctype="multipart/form-data" autocomplete="off">
        <select class="controls"  name="marca" required>
         
            <option>CHEVROLET</option>

            <option>SUZUKI</option>
            
            <option>RENAULT</option>

            <option>BMW</option>

            <option>AUDI</option>

            <option>TOYOTA</option>

            <option>FORD</option>

            <option>MERCEDES-BENZ</option>

            
            
            
       
            
            </select>
            <input class="controls" style="text-transform:uppercase;" type="text" name="linea" id="linea" placeholder="Ingrese la linea del vehiculo" required>
    <input class="controls" type="text" name="modelo" id="modelo" placeholder="Ingrese el modelo del vehiculo" required>
    <select class="controls"  name="color" required>
         
        <option>NEGRO</option>
        
        <option>ROJO</option>

        <option>AZUL</option>

        <option>PLATEADO</option>

        <option>VERDE</option>

        <option>GRIS</option>

        <option>BEIGE</option>
        <option>BLANCO</option>

        
        
        
   
        
        </select>
    <select class="controls"  name="categoria" required>
        <% Datos.forEach(function (Datos){ %>
        <option value="<%=Datos.catid%>"> <%=Datos.catipo%></option>
        
        <%})%>
        
   
        
        </select>
    <input class="controls" type="number" name="precio" id="precio" placeholder="Ingrese el valor del vehiculo" required>
    <input class="controls" style="text-transform:uppercase;" type="text" name="placa" id="placa" placeholder="Ingrese la placa del vehiculo" required>

    <select class="controls"  name="estado">

      <option>Nuevo</option>
      
      <option>Usado</option>
      
 
      
      </select>
      <input class="controls" type="file" name="img" id="IMG" placeholder="Ingrese la placa del vehiculo"required>
    <input class="botons" type="submit" value="Vender ">
  </form>
  </section>